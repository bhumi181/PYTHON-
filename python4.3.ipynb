   "source": [
    "#PROGRAM TO COMPUTE FREQUENCY OF WORDS FROM THE INPUT #3\n",
    "bmw = input().split()\n",
    "dict = {}\n",
    "for i in bmw:\n",
    "    i = dict.setdefault(i,bmw.count(i))\n",
    "dict = sorted(dict.items())\n",
    "for i in dict:\n",
    "    print(\"%s:%d\"%(i[0],i[1]))\n"
